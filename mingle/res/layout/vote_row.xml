<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false"
    android:orientation="horizontal" >
    

    <LinearLayout android:layout_weight="1" android:layout_height="60dp"
        android:layout_width="fill_parent" android:layout_margin="@dimen/small_margin">
        <RelativeLayout
            android:orientation="vertical"
            android:id="@+id/female_vote_rl"
            android:background="@drawable/vote_profile"
            android:padding="1dp"
            android:onClick="showProfile"
            android:tag="front"
            style="@style/MyListFrontContent">
		    	<ImageView
		                style="@style/ChatRoomRank"
		                android:id="@+id/top_female_rank"/>
		    	<ImageView
		                style="@style/ChatRoomImage"
		                android:id="@+id/top_female_image"/>
		    	
		        
		        <TextView
		                android:layout_width="fill_parent"
		                android:layout_height="wrap_content"
		                android:layout_toRightOf="@id/top_female_image"
		                style="@style/MyListTitle"
		                android:id="@+id/top_female_name"/>
        </RelativeLayout>
    </LinearLayout>
    <LinearLayout android:layout_weight="1" 
        android:layout_height="60dp" android:layout_width="fill_parent" android:layout_margin="@dimen/small_margin">
        <RelativeLayout
            android:orientation="vertical"
            android:id="@+id/male_vote_rl"
            android:padding="1dp"
            android:background="@drawable/vote_profile"
            android:tag="front"
            android:onClick="showProfile"
            style="@style/MyListFrontContent">
        			<ImageView
		                style="@style/ChatRoomRank"
		                android:id="@+id/top_male_rank"/>
        			<ImageView
              			  style="@style/ChatRoomImage"
              			  android:id="@+id/top_male_image"/>
        			
        
			        <TextView
			                android:layout_width="fill_parent"
			                android:layout_height="wrap_content"
			                android:layout_toRightOf="@id/top_male_image"
			                style="@style/MyListTitle"
			                android:id="@+id/top_male_name"/>
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>
