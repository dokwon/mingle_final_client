<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:baselineAligned="false"
    android:orientation="horizontal" >
    

    <LinearLayout android:layout_weight="1" android:layout_height="60dp"
        android:layout_width="fill_parent" android:layout_margin="@dimen/small_margin">
        <FrameLayout
            android:orientation="vertical"
            android:id="@+id/pop_user_rl"
            android:background="@drawable/vote_profile"
            android:layout_marginLeft="@dimen/small_margin"
            android:tag="front"
            style="@style/MyListFrontContent">
		    	
		    	<ImageView
		                style="@style/VoteProfileImage"
		                android:id="@+id/pop_user_image"/>
		    	<ImageView
		                style="@style/ChatRoomRank"
		                android:id="@+id/pop_user_rank"/>
		        
		        <TextView
		                android:layout_width="fill_parent"
		                android:layout_height="wrap_content"
		                android:layout_toRightOf="@+id/pop_user_image"
		                android:layout_marginLeft="70dp"
		                style="@style/MyListTitle"
		                android:id="@+id/pop_user_name"/>
        </FrameLayout>
    </LinearLayout>
